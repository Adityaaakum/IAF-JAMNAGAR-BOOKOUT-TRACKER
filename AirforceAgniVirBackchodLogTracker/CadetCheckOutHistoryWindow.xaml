<Window x:Class="AirforceAgniVirBackchodLogTracker.CadetCheckOutHistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirforceAgniVirBackchodLogTracker"
        mc:Ignorable="d"
        Title="Agniveer Book Out History" Height="450" Width="800">
    
    <Grid x:Name="CheckOutHistory">
        <DataGrid x:Name="dataGrid_CheckoutHistory" AutoGenerateColumns="True" HorizontalContentAlignment="Stretch"  CanUserSortColumns="True"  MinColumnWidth="90"  IsReadOnly="True"  AutoGeneratingColumn="dataGrid_AutoGeneratingColumn" Background="Transparent"/>
        <Button x:Name="DeleteBookOutHistory_Button" Content="Delete BookOut History"  HorizontalAlignment="Stretch" Height="20" Margin="10,20,10,40" Click="Button_Click"  Background="Red" Foreground="White"  BorderThickness="2"  VerticalAlignment="Bottom"/>
        <Button Content="Print"  HorizontalAlignment="Stretch" Height="20" Margin="10,0,10,10"   Background="DodgerBlue" Foreground="White" Click="PrintCheckoutHistory" BorderThickness="2"  VerticalAlignment="Bottom" />
    </Grid>
</Window>
